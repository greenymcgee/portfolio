{
  "name": "houston-c-green-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "dev:db:migrate": "set -a && . ./.env && npx prisma migrate deploy",
    "dev:db:seed": "set -a && . ./.env && npx prisma db seed",
    "dev:db:reset": "set -a && . ./.env && npx prisma migrate reset",
    "build": "npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint .",
    "postinstall": "npx prisma generate",
    "spellcheck": "cspell \"/**/*.*\"",
    "test": "set -a && . ./.env.test && vitest",
    "test:ci": "vitest run --coverage",
    "test:coverage": "set -a && . ./.env.test && vitest run --coverage",
    "test:db:migrate": "set -a && . ./.env.test && npx prisma migrate deploy",
    "test:db:reset": "set -a && . ./.env.test && npx prisma migrate reset",
    "tsc": "tsc",
    "validate": "npm run tsc && npm run lint && npm run spellcheck && npm run test:coverage"
  },
  "dependencies": {
    "@greenymcgee/summon": "^0.2.2",
    "@greenymcgee/typescript-utils": "^0.1.8",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "lucide-react": "^0.563.0",
    "neverthrow": "^8.2.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "pino": "^10.3.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@faker-js/faker": "^10.3.0",
    "@greenymcgee/next-eslint-config": "^0.1.6",
    "@greenymcgee/tailwind-prettier-config": "^0.1.1",
    "@greenymcgee/utility-types": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "cspell": "^9.6.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fishery": "^2.4.0",
    "jsdom": "^28.0.0",
    "msw": "^2.12.9",
    "next-router-mock": "^1.0.5",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3"
  }
}
